@import url('https://fonts.googleapis.com/css?family=Oswald');

body {
    font-family: 'Oswald', sans-serif;
background-color: #D8D8D8;

}

footer {
   position: relative;
    background-color:#1f1f1f;
bottom: -800px;
}

#aboutus{
    position:relative;
    top: 425px;
    padding: 40px;
    padding-left: 40px;
    padding-right: 40px;
    background-color: #01A4CD;
    text-transform:uppercase;
    font-size: 22pt;
    
    color:white;
}

ul.navigationbar {
    list-style-type: none;
    margin: 0;
    padding: 0;
    overflow: hidden;
    background-color: #1f1f1f;
    position: fixed;
    top: 0;
    width: 100%;
    z-index: 1;
    
}

li#title{
    display: block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
    float:right;
    
}

li.navigationbar{
    float:left;
}

li a#navbar{
    display: block;
    color: white;
    text-align: center;
    padding: 14px 16px;
    text-decoration: none;
  
}

li a#navbar:hover{
    background-color: gray;
}

.active{
    background-color: gray;
}

h1, h2 {
    color: #6c6c6c;
    font-size: 26pt;
   
}

h3, h4, h5, h6 {
    color: #6c6c6c;
    font-size: 15pt;
}
h3:hover, h4:hover, h5:hover, h6:hover {
    color: darkgray;
}




#missionstatementouter{
    position: absolute;
    height: 400px;
    width: 100%;
    background-image: linear-gradient(
        rgba(0,0,0,0.2),
        rgba(0,0,0,0.2)
    ),
        url(images-liam/Cocktails.jpg);
    
    background-position: center;
    background-size: 100%;
    background-repeat: no-repeat; 
    
    
    
    padding: 150px;
    
}

#FAQsection{
    position: relative;
    text-align: justify;
    bottom:-500px;
    color: #6c6c6c;
    
}


.FAQ {  cursor:hand; 
        cursor:pointer;
        color:white;
        width:50%; 
   
        
    }

 .FAA { display:none; 
        color:#6c6c6c;
     
